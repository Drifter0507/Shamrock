		instances.TextLabel_4.Font = Enum.Font.Gotham
		instances.TextLabel_4.RichText = true
		instances.TextLabel_4.Text = 'Prompt:'
		instances.TextLabel_4.TextColor3 = Color3.new(0.952941, 0.952941, 0.952941)
		instances.TextLabel_4.TextSize = 16
		instances.TextLabel_4.TextWrapped = true
		instances.TextLabel_4.TextXAlignment = Enum.TextXAlignment.Left
		instances.TextLabel_4.BackgroundColor3 = Color3.new(1, 1, 1)
		instances.TextLabel_4.BackgroundTransparency = 1
		instances.TextLabel_4.BorderSizePixel = 0
		instances.TextLabel_4.Position = UDim2.new(0.035062216222286224, 0, 0, 0)
		instances.TextLabel_4.Size = UDim2.new(0, 201, 0, 23)

		return instances.Frame_1
	end
	local Notifications = game.Players.LocalPlayer.PlayerGui[_G.FewofiwegngreRG452]:FindFirstChild("Notifications"):GetChildren()
	if #Notifications >= 5 then
		Notifications[1]:TweenPosition(UDim2.new(1.5, 0, Notifications[1].Position.Y.Scale, 0),"InOut","Linear",0.2,true);wait(0.2)
		Notifications[1]:Destroy()
		for i,v in pairs(Notifications) do if v ~= nil then
				v:TweenPosition(UDim2.new(1, -209, v.Position.Y.Scale - 0.1, 0),"InOut","Linear",0.2,true)
			end
		end
		local frame = createUI(tostring(#Notifications+1))
		if not isPrompt then
			for i, v in pairs(frame:GetDescendants()) do
				if v:IsA("TextLabel") and v.Text == "Prompt:" then
					v.Text = "Notification:"
				end
				if v:IsA("TextLabel") and v.Name == "MessageHere" then
					v.Size = UDim2.new(0, 209,0, 61)
				end
				if v:IsA("TextButton") and v.Name == "Yes" or v.Name == "No" then
					v.Visible = false
				end
			end
		end
		frame.Position = UDim2.new(1, 209,0.757, 0)
		wait(0.1)
		frame:TweenPosition(UDim2.new(1, -209,0.757, 0),"InOut","Linear",0.2,true)
		wait(0.4)
	else
		for i,v in pairs(Notifications) do
			v:TweenPosition(UDim2.new(1, -209, v.Position.Y.Scale - 0.1, 0),"InOut","Linear",0.2,true)
		end
		local frame = createUI(tostring(#Notifications+1))
		if not isPrompt then
			for i, v in pairs(frame:GetDescendants()) do
				if v:IsA("TextLabel") and v.Text == "Prompt:" then
					v.Text = "Notification:"
				end
				if v:IsA("TextLabel") and v.Name == "MessageHere" then
					v.Size = UDim2.new(0, 209,0, 61)
				end
				if v:IsA("TextButton") and v.Name == "Yes" or v.Name == "No" then
					v.Visible = false
				end
			end
		end
		frame.Position = UDim2.new(1, 209,0.757, 0)
		wait(0.1)
		frame:TweenPosition(UDim2.new(1, -209,0.757, 0),"InOut","Linear",0.2,true)
		wait(0.4)


		for i, v in pairs(frame:GetDescendants()) do
			if v:IsA("TextButton") and v:FindFirstChild("Shadow") then
				v.MouseEnter:Connect(function()
					game.TweenService:Create(v:FindFirstChild("Shadow"), TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.In), {
						BackgroundTransparency = 0.8
					}):Play()
				end)
				v.MouseLeave:Connect(function()
					game.TweenService:Create(v:FindFirstChild("Shadow"), TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.In), {
						BackgroundTransparency = 1
					}):Play()
				end)
				v.MouseButton1Click:Connect(function()
					callback(tostring(v.Name))
					frame:TweenPosition(UDim2.new(1.5, 0, frame.Position.Y.Scale, 0),"InOut","Linear",0.2,true);wait(0.2)
					frame:Destroy()
				end)
			end
		end


		if DoesGoAway then
			delay(NotificationDuration,function()
				local pos = frame.Position.Y.Scale
				frame:TweenPosition(UDim2.new(1.5, 0, frame.Position.Y.Scale, 0),"InOut","Linear",0.2,true);wait(0.2)
				frame:Destroy()
				--wait(0.1)
				--for i,v in pairs(Notifications) do
				--	v:TweenPosition(UDim2.new(1, -209, v.Position.Y.Scale + pos + 0.195, 0),"InOut","Linear",0.2,true)
				--end
			end)	
		end
	end
end


