				dropdown.TextButton_1.AutoButtonColor = false

				dropdown.UICorner_1.Parent = dropdown.TextButton_1
				dropdown.UICorner_1.CornerRadius = UDim.new(0, 2)

				dropdown.Frame_2.Parent = dropdown.TextButton_1
				dropdown.Frame_2.BackgroundColor3 = Color3.new(0, 0, 0)
				dropdown.Frame_2.BackgroundTransparency = 1
				dropdown.Frame_2.BorderColor3 = Color3.new(0.105882, 0.164706, 0.207843)
				dropdown.Frame_2.BorderSizePixel = 0
				dropdown.Frame_2.Size = UDim2.new(0, 240, 0, 20)
				dropdown.Frame_2.ZIndex = 2
				dropdown.Frame_2.Name = 'Shadow'

				dropdown.TextLabel_1.Parent = dropdown.TextButton_1
				dropdown.TextLabel_1.Font = Enum.Font.Gotham
				dropdown.TextLabel_1.Text = btnText
				dropdown.TextLabel_1.TextColor3 = CurrentTheme.TextColor
				dropdown.TextLabel_1.TextSize = 15
				dropdown.TextLabel_1.TextXAlignment = Enum.TextXAlignment.Left
				dropdown.TextLabel_1.BackgroundColor3 = Color3.new(1, 1, 1)
				dropdown.TextLabel_1.BackgroundTransparency = 1
				dropdown.TextLabel_1.BorderSizePixel = 0
				dropdown.TextLabel_1.Position = UDim2.new(0, 3, 0, 0)
				dropdown.TextLabel_1.Size = UDim2.new(0, 225, 0, 19)

				dropdown.ImageLabel_1.Parent = dropdown.TextButton_1
				dropdown.ImageLabel_1.Image = 'rbxassetid://3926307971'
				dropdown.ImageLabel_1.ImageRectOffset = Vector2.new(164, 524)
				dropdown.ImageLabel_1.ImageRectSize = Vector2.new(36, 36)
				dropdown.ImageLabel_1.BackgroundColor3 = Color3.new(1, 1, 1)
				dropdown.ImageLabel_1.BackgroundTransparency = 1
				dropdown.ImageLabel_1.Position = UDim2.new(0.9129999876022339, 0, 0, 0)
				dropdown.ImageLabel_1.Rotation = 0
				dropdown.ImageLabel_1.Size = UDim2.new(0, 19, 0, 19)
				dropdown.ImageLabel_1.Name = 'arrow'

				dropdown.Frame_3.Parent = dropdown.TextButton_1
				dropdown.Frame_3.BackgroundColor3 = Color3.new(1, 1, 1)
				dropdown.Frame_3.BackgroundTransparency = 1
				dropdown.Frame_3.BorderSizePixel = 0
				dropdown.Frame_3.Position = UDim2.new(0, 2, 0, 20)
				dropdown.Frame_3.Size = UDim2.new(0, 236, 0, 0)
