		instances.Frame_8.Size = UDim2.new(1, 0, 1, 0)
		instances.Frame_8.ZIndex = 2
		instances.Frame_8.Name = 'Shadow'

		instances.TextLabel_1.Parent = instances.TextButton_1
		instances.TextLabel_1.Font = Enum.Font.Gotham
		instances.TextLabel_1.Text = tostring(green) or "Yes"
		instances.TextLabel_1.TextColor3 = Color3.new(0.952941, 0.952941, 0.952941)
