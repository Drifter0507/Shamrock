				taken = true
			else
				TabInstances.TextButton_1.TextColor3 = Color3.fromRGB(207, 207, 207)
				TabInstances.ScrollingFrame_1.Visible = false
				TabInstances.PageFrame.BackgroundColor3 = Color3.new(0, 0.631373, 0.294118)
				TabInstances.PageFrame.Position = UDim2.new(0, 0, 0, 24)
				TabInstances.PageFrame.Size = UDim2.new(0, 0, 0.07407407462596893, 0)
			end
		else
			TabInstances.TextButton_1.TextColor3 = Color3.fromRGB(207, 207, 207)
			TabInstances.ScrollingFrame_1.Visible = false
			TabInstances.PageFrame.BackgroundColor3 = Color3.new(0, 0.631373, 0.294118)
			TabInstances.PageFrame.Position = UDim2.new(0, 0, 0, 24)
			TabInstances.PageFrame.Size = UDim2.new(0, 0, 0.07407407462596893, 0)
		end



		local newPage = TabInstances.ScrollingFrame_1

		TabInstances.UICorner_1.Parent = instances.PageFrame
		TabInstances.UICorner_1.CornerRadius = UDim.new(0, 4)
		TabInstances.TextButton_1.MouseButton1Click:Connect(function()
			for i,v in next, instances.Folder_1:GetChildren() do
				v.Visible = false
			end 
			TabInstances.ScrollingFrame_1.Visible = true

			--Animations Below  -- Optional
			for i,v in pairs(instances.Frame_8:GetChildren()) do
