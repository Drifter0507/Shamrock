				ColorPickerInstances.TextLabel_1.TextXAlignment = Enum.TextXAlignment.Left
				ColorPickerInstances.TextLabel_1.BackgroundColor3 = Color3.new(1, 1, 1)
				ColorPickerInstances.TextLabel_1.BackgroundTransparency = 1
				ColorPickerInstances.TextLabel_1.Position = UDim2.new(0, -3, 0, 9)
				ColorPickerInstances.TextLabel_1.Size = UDim2.new(0, 163, 0, 22)

				local opened = false
				local old = ColorPickerInstances.Frame_1.Size.Y.Offset

				ColorPickerInstances.TextButton_1.MouseButton1Click:Connect(function()
					if opened then
						opened = false
						game.TweenService:Create(ColorPickerInstances.Frame_4, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.In), {
							Size = UDim2.new(0, 236,0, 0)
						}):Play()
						game.TweenService:Create(ColorPickerInstances.Frame_1, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.In), {
							Size = UDim2.new(0, 240, 0, old)
						}):Play()
						wait(0.1)
						ColorPickerInstances.Frame_4.Visible = false
					else
						opened = true
						ColorPickerInstances.Frame_4.Visible = true
						game.TweenService:Create(ColorPickerInstances.Frame_4, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.In), {
							Size = UDim2.new(0, 236,0, 25)
						}):Play()
						game.TweenService:Create(ColorPickerInstances.Frame_1, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.In), {
							Size = UDim2.new(0, 240, 0, old + 25)
						}):Play()
					end
				end)


				local Player = game.Players.LocalPlayer
				local UserInputService = game:GetService('UserInputService')
				local ColorShower = ColorPickerInstances.TextButton_1
				local PickerArea = ColorPickerInstances.Frame_6
				local Picker = ColorPickerInstances.Frame_7
				local Gradient = ColorPickerInstances.UIGradient_1
				local selecting = false

				local function getColor(percentage, ColorKeyPoints)
					local closestToLeft = ColorKeyPoints[1]
					local closestToRight = ColorKeyPoints[#ColorKeyPoints]
					local LocalPercentage = .5
					local color = closestToLeft.Value

					for i=1,#ColorKeyPoints-1 do
						if (ColorKeyPoints[i].Time <= percentage) and (ColorKeyPoints[i+1].Time >= percentage) then
							closestToLeft = ColorKeyPoints[i]
							closestToRight = ColorKeyPoints[i+1]
							LocalPercentage = (percentage-closestToLeft.Time)/(closestToRight.Time-closestToLeft.Time)
							color = closestToLeft.Value:lerp(closestToRight.Value,LocalPercentage)
							return color
						end
					end
					return color
				end
				
				local selecting
				local function beginSelection()
					selecting = true
					local ColorKeyPoints = Gradient.Color.Keypoints
					repeat wait()
						local minXPos = PickerArea.AbsolutePosition.X
						local maxXPos = minXPos+PickerArea.AbsoluteSize.X

						local xPixelSize = maxXPos-minXPos

						local mouseX = UserInputService:GetMouseLocation().X

						if mouseX<minXPos then
							mouseX = minXPos
						elseif mouseX>maxXPos then
							mouseX = maxXPos
						end

						local xPos = (mouseX-minXPos)/xPixelSize

						Picker.Position = UDim2.new(xPos,0,0,0)
						ColorShower.BackgroundColor3 = getColor(xPos,ColorKeyPoints)
						callback(getColor(xPos,ColorKeyPoints))
						local Color = ColorSequence.new{
							ColorSequenceKeypoint.new(0,Color3.fromRGB(255,255,255)),
							ColorSequenceKeypoint.new(0.5,getColor(xPos,ColorKeyPoints)),
							ColorSequenceKeypoint.new(1,Color3.fromRGB(0,0,0))
						}	
					until not selecting
				end

				local function endSelection()
					selecting = false
				end

				local function inputBegan(input)
					if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
						beginSelection()
					end
				end
				PickerArea.InputBegan:Connect(inputBegan)

				local function inputEnded(input)
					if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
						endSelection()
					end
				end
				PickerArea.InputEnded:Connect(inputEnded)


			end

			return ElementHandler

		end

		return SectionHandler


	end


	return TabHandler

end

return WindowTable


