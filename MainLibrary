					Shadow.Name = "Shadow"
					Shadow.Parent = TextButton
					Shadow.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
					Shadow.BackgroundTransparency = 1.000
					Shadow.BorderColor3 = Color3.fromRGB(27, 42, 53)
					Shadow.BorderSizePixel = 0
					Shadow.Size = UDim2.new(1, 0, 1, 0)
					Shadow.ZIndex = 2

