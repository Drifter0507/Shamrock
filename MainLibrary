	function Code_LocalScript_1() --LocalScript
		local script = instances.LocalScript_1

		local UserInputService = game:GetService("UserInputService")

		local gui = script.Parent

		local dragging
		local dragInput
		local dragStart
		local startPos

		local function update(input)
			local delta = input.Position - dragStart
			gui.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
		end

		gui.InputBegan:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
				dragging = true
				dragStart = input.Position
				startPos = gui.Position

				input.Changed:Connect(function()
					if input.UserInputState == Enum.UserInputState.End then
						dragging = false
					end
				end)
			end
		end)

		gui.InputChanged:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
				dragInput = input
			end
		end)

		UserInputService.InputChanged:Connect(function(input)
			if input == dragInput and dragging then
				update(input)
			end
		end)
	end
	coroutine.wrap(Code_LocalScript_1)()


	local TabHandler = {}

	function TabHandler:CreateTab(config)
		local tabname = config.Title or "New Tab"
		local scrollingbar = config.ScrollBar

		--local newPage = Instance.new("ScrollingFrame") 
		--local elementsListing = Instance.new("UIListLayout") 
		local uiPadding = Instance.new("UIPadding")

		local TabInstances = {
			["PageFrame"] = Instance.new("Frame"),
			["TextButton_1"] = Instance.new("TextButton"),
			["UICorner_1"] = Instance.new("UICorner"),
			["UIListLayout_1"] = Instance.new("UIListLayout"),
			["ScrollingFrame_1"] = Instance.new("ScrollingFrame")
		}

		TabInstances.ScrollingFrame_1.BackgroundColor3 = CurrentTheme.BackgroundSubColor

		TabInstances.TextButton_1.Parent = instances.Frame_8
		TabInstances.TextButton_1.Font = Enum.Font.Gotham
		TabInstances.TextButton_1.Text = tabname
		--TabInstances.TextButton_1.TextColor3 = Color3.new(0.952941, 0.952941, 0.952941)
