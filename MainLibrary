					game.TweenService:Create(buttoninstances.TextButton_1:FindFirstChild("Shadow"), TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.In), {
						BackgroundTransparency = 0.8
					}):Play()
				end)
				buttoninstances.TextButton_1.MouseLeave:Connect(function()
					game.TweenService:Create(buttoninstances.TextButton_1:FindFirstChild("Shadow"), TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.In), {
						BackgroundTransparency = 1
					}):Play()
				end)


				buttoninstances.UICorner_1.Parent = buttoninstances.TextButton_1
				buttoninstances.UICorner_1.CornerRadius = UDim.new(0, 2)

				buttoninstances.ImageButton_1.Parent = buttoninstances.TextButton_1
				buttoninstances.ImageButton_1.Image = 'rbxassetid://3926305904'
				buttoninstances.ImageButton_1.ImageColor3 = CurrentTheme.TextColor
				buttoninstances.ImageButton_1.ImageRectOffset = Vector2.new(204, 964)
				buttoninstances.ImageButton_1.ImageRectSize = Vector2.new(36, 36)
				buttoninstances.ImageButton_1.Active = false
				buttoninstances.ImageButton_1.BackgroundTransparency = 1
				buttoninstances.ImageButton_1.Position = UDim2.new(0, 220, 0, 1)
				buttoninstances.ImageButton_1.Selectable = false
				buttoninstances.ImageButton_1.Size = UDim2.new(0, 16, 0, 16)
				buttoninstances.ImageButton_1.ZIndex = 2
				buttoninstances.ImageButton_1.AutoButtonColor = false
				buttoninstances.ImageButton_1.Archivable = false
				buttoninstances.ImageButton_1.Name = 'mouse'

				buttoninstances.TextLabel_1.Parent = buttoninstances.TextButton_1
				buttoninstances.TextLabel_1.Font = Enum.Font.Gotham
				buttoninstances.TextLabel_1.Text = btnText
				buttoninstances.TextLabel_1.TextColor3 = CurrentTheme.TextColor
				buttoninstances.TextLabel_1.TextSize = 15
				buttoninstances.TextLabel_1.TextXAlignment = Enum.TextXAlignment.Left
				buttoninstances.TextLabel_1.BackgroundColor3 = Color3.new(1, 1, 1)
				buttoninstances.TextLabel_1.BackgroundTransparency = 1
				buttoninstances.TextLabel_1.BorderSizePixel = 0
				buttoninstances.TextLabel_1.Position = UDim2.new(0, 7, 0, 0)
				buttoninstances.TextLabel_1.Size = UDim2.new(0, 232, 0, 19)

				buttoninstances.Frame_2.Parent = buttoninstances.TextButton_1
				buttoninstances.Frame_2.BackgroundColor3 = Color3.new(0, 0, 0)
				buttoninstances.Frame_2.BackgroundTransparency = 1
				buttoninstances.Frame_2.BorderColor3 = Color3.new(0.105882, 0.164706, 0.207843)
				buttoninstances.Frame_2.BorderSizePixel = 0
				buttoninstances.Frame_2.Size = UDim2.new(0, 240, 0, 20)
				buttoninstances.Frame_2.ZIndex = 2
				buttoninstances.Frame_2.Name = 'Shadow'

				buttoninstances.TextButton_1.MouseButton1Click:Connect(function()
					callback()
				end)

				function ButtonFunction:UpdateButton(str)
					str = tostring(str) or "Button"
					buttoninstances.TextLabel_1.Text = tostring(str)
				end
				return ButtonFunction
			end

			function ElementHandler:CreateToggle(config)
				local btnText = config.Text or config.text or "Toggle"
				local callback = config.callback or config.Callback or function() end   
				local OrderNumber = config.Order or config.order or 20

				local toggleinstance = {
					["TextLabel_1"] = Instance.new("TextLabel"),
					["ImageButton_1"] = Instance.new("ImageButton"),
					["Frame_2"] = Instance.new("Frame"),
					["UICorner_1"] = Instance.new("UICorner"),
					["Frame_1"] = Instance.new("Frame"),
				}

				toggleinstance.Frame_1.Parent = sectioninstances.Frame_3
				toggleinstance.Frame_1.BackgroundColor3 = Color3.new(1, 1, 1)
				toggleinstance.Frame_1.BackgroundTransparency = 1
				toggleinstance.Frame_1.BorderColor3 = Color3.new(0.105882, 0.164706, 0.207843)
				toggleinstance.Frame_1.BorderSizePixel = 0
				toggleinstance.Frame_1.Position = UDim2.new(0, 0, 0, 93)
				toggleinstance.Frame_1.Size = UDim2.new(0, 240, 0, 36)
				toggleinstance.Frame_1.LayoutOrder = OrderNumber
				toggleinstance.Frame_1.Name = 'Toggler'

				toggleinstance.Frame_2.Parent = toggleinstance.Frame_1
				toggleinstance.Frame_2.BackgroundColor3 = Color3.new(0.0588235, 0.0588235, 0.0588235)
				toggleinstance.Frame_2.BorderColor3 = Color3.new(0.27451, 0.27451, 0.27451)
				toggleinstance.Frame_2.BorderSizePixel = 2
				toggleinstance.Frame_2.Position = UDim2.new(0, 0, 0.3125, 0)
				toggleinstance.Frame_2.Size = UDim2.new(0, 16, 0, 16)
				toggleinstance.Frame_2.ZIndex = 5
				toggleinstance.Frame_2.Name = 'Outline'

				toggleinstance.ImageButton_1.Parent = toggleinstance.Frame_2
				toggleinstance.ImageButton_1.Image = 'rbxassetid://3926305904'
				toggleinstance.ImageButton_1.ImageRectOffset = Vector2.new(312, 4)
				toggleinstance.ImageButton_1.ImageRectSize = Vector2.new(24, 24)
				toggleinstance.ImageButton_1.BackgroundColor3 = CurrentTheme.ThemeColor
				toggleinstance.ImageButton_1.LayoutOrder = 1
				toggleinstance.ImageButton_1.Position = UDim2.new(-0.0625, 0, -0.0625, 0)
				toggleinstance.ImageButton_1.Size = UDim2.new(0, 18, 0, 18)
				toggleinstance.ImageButton_1.ZIndex = 2
				toggleinstance.ImageButton_1.Name = 'Toggle'

				toggleinstance.UICorner_1.Parent = toggleinstance.ImageButton_1
				toggleinstance.UICorner_1.CornerRadius = UDim.new(0, 2)

				toggleinstance.TextLabel_1.Parent = toggleinstance.Frame_1
				toggleinstance.TextLabel_1.Font = Enum.Font.Gotham
				toggleinstance.TextLabel_1.Text = config.Title
				toggleinstance.TextLabel_1.TextColor3 = CurrentTheme.TextColor
				toggleinstance.TextLabel_1.TextSize = 14
				toggleinstance.TextLabel_1.TextXAlignment = Enum.TextXAlignment.Left
				toggleinstance.TextLabel_1.BackgroundColor3 = Color3.new(1, 1, 1)
				toggleinstance.TextLabel_1.BackgroundTransparency = 1
				toggleinstance.TextLabel_1.Position = UDim2.new(0.12916666269302368, 0, 0.29797980189323425, 0)
				toggleinstance.TextLabel_1.Size = UDim2.new(0, 76, 0, 18)


				local Shadow = Instance.new("Frame")

				Shadow.Name = "Shadow"
				Shadow.Parent = toggleinstance.Frame_2
				Shadow.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
				Shadow.BackgroundTransparency = 0.700
				Shadow.BorderColor3 = Color3.fromRGB(27, 42, 53)
				Shadow.Position = UDim2.new(-0.125, 0,-0.063, 0)
				Shadow.BorderSizePixel = 0
				Shadow.Size = UDim2.new(1.187, 0,1.155, 0)
				Shadow.ZIndex = 2
				Shadow.BackgroundTransparency = 1


				if config.Default and config.Default == true then
					tog = true
					toggleinstance.ImageButton_1.BackgroundTransparency = 0
					toggleinstance.ImageButton_1.ImageTransparency = 0
				else
					tog = false
					toggleinstance.ImageButton_1.BackgroundTransparency = 1
					toggleinstance.ImageButton_1.ImageTransparency = 1
				end
				if not config.Default or config.Default == false then
					tog = false
					toggleinstance.ImageButton_1.BackgroundTransparency = 1
					toggleinstance.ImageButton_1.ImageTransparency = 1
				end
				toggleinstance.ImageButton_1.MouseEnter:Connect(function()
					game.TweenService:Create(toggleinstance.Frame_2:FindFirstChild("Shadow"), TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.In), {
						BackgroundTransparency = 0.8
					}):Play()
				end)
				toggleinstance.ImageButton_1.MouseLeave:Connect(function()
					game.TweenService:Create(toggleinstance.Frame_2:FindFirstChild("Shadow"), TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.In), {
						BackgroundTransparency = 1
					}):Play()
				end)				

				toggleinstance.ImageButton_1.MouseButton1Click:Connect(function()
					tog = not tog
					if tog then 
						game.TweenService:Create(toggleinstance.ImageButton_1, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.In), {
							BackgroundTransparency = 0
						}):Play()
						game.TweenService:Create(toggleinstance.ImageButton_1, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.In), {
							ImageTransparency = 0
						}):Play()
					else
						game.TweenService:Create(toggleinstance.ImageButton_1, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.In), {
							BackgroundTransparency = 1
						}):Play()
						game.TweenService:Create(toggleinstance.ImageButton_1, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.In), {
							ImageTransparency = 1
						}):Play()
					end
					callback(tog)
				end)
			end
			function ElementHandler:CreateSlider(config)
				local btnText = config.Text or config.text or "Slider"
				local callback = config.callback or config.Callback or function() end   
				local Min = config.Min or config.min or 0
				local Max = config.Max or config.max or 100
				local Default = config.Default or config.default or 16

				local OrderNumber = config.Order or config.order or 20


				local sliderinstances = {
					["TextLabel_2"] = Instance.new("TextLabel"),
					["TextLabel_1"] = Instance.new("TextLabel"),
					["Frame_3"] = Instance.new("Frame"),
					["UICorner_2"] = Instance.new("UICorner"),
					["Frame_2"] = Instance.new("Frame"),
					["UICorner_1"] = Instance.new("UICorner"),
					["TextButton_1"] = Instance.new("TextButton"),
					["Frame_4"] = Instance.new("Frame"),
					["Frame_1"] = Instance.new("Frame"),
					["UICorner_3"] = Instance.new("UICorner"),
				}

				sliderinstances.Frame_1.Parent = sectioninstances.Frame_3
				sliderinstances.Frame_1.BackgroundColor3 = Color3.new(1, 1, 1)
				sliderinstances.Frame_1.BackgroundTransparency = 1
				sliderinstances.Frame_1.BorderColor3 = Color3.new(0.105882, 0.164706, 0.207843)
				sliderinstances.Frame_1.Position = UDim2.new(0, 9, 0, 4)
				sliderinstances.Frame_1.Size = UDim2.new(0, 240, 0, 40)
				sliderinstances.Frame_1.LayoutOrder = OrderNumber
				sliderinstances.Frame_1.Name = 'Slider'

				sliderinstances.TextLabel_1.Parent = sliderinstances.Frame_1
				sliderinstances.TextLabel_1.Font = Enum.Font.Gotham
				sliderinstances.TextLabel_1.Text = '45'
				sliderinstances.TextLabel_1.TextColor3 = CurrentTheme.TextColor
				sliderinstances.TextLabel_1.TextSize = 12
				sliderinstances.TextLabel_1.TextXAlignment = Enum.TextXAlignment.Right
				sliderinstances.TextLabel_1.BackgroundColor3 = Color3.new(1, 1, 1)
				sliderinstances.TextLabel_1.BackgroundTransparency = 1
				sliderinstances.TextLabel_1.Position = UDim2.new(0.8291666507720947, 0, -0.05000000074505806, 0)
				sliderinstances.TextLabel_1.Size = UDim2.new(0, 35, 0, 28)
				sliderinstances.TextLabel_1.Name = 'Number'

				sliderinstances.TextLabel_2.Parent = sliderinstances.Frame_1
				sliderinstances.TextLabel_2.Font = Enum.Font.Gotham
				sliderinstances.TextLabel_2.Text = btnText
				sliderinstances.TextLabel_2.TextColor3 = CurrentTheme.TextColor
				sliderinstances.TextLabel_2.TextSize = 12
				sliderinstances.TextLabel_2.TextXAlignment = Enum.TextXAlignment.Left
				sliderinstances.TextLabel_2.BackgroundColor3 = Color3.new(1, 1, 1)
				sliderinstances.TextLabel_2.BackgroundTransparency = 1
				sliderinstances.TextLabel_2.Position = UDim2.new(-0.004183955956250429, 0, -0.05000000074505806, 0)
				sliderinstances.TextLabel_2.Size = UDim2.new(0, 183, 0, 28)


				sliderinstances.TextButton_1.Parent = sliderinstances.Frame_1
				sliderinstances.TextButton_1.Font = Enum.Font.SourceSans
				sliderinstances.TextButton_1.Text = ''
				sliderinstances.TextButton_1.TextColor3 = Color3.new(0, 0, 0)
				sliderinstances.TextButton_1.TextSize = 14
				sliderinstances.TextButton_1.BackgroundColor3 = Color3.new(0.27451, 0.27451, 0.27451)
				sliderinstances.TextButton_1.BorderSizePixel = 0
				sliderinstances.TextButton_1.Position = UDim2.new(-0.00800000037997961, 0, 0.675000011920929, 0)
				sliderinstances.TextButton_1.Size = UDim2.new(0, 240, 0, 13)

				sliderinstances.UICorner_1.Parent = sliderinstances.TextButton_1
				sliderinstances.UICorner_1.CornerRadius = UDim.new(0, 2)

				sliderinstances.UICorner_2.Parent = sliderinstances.TextButton_1
				sliderinstances.UICorner_2.CornerRadius = UDim.new(0, 2)

				sliderinstances.Frame_3.Parent = sliderinstances.TextButton_1
				sliderinstances.Frame_3.BackgroundColor3 = Color3.new(0.117647, 0.117647, 0.117647)
				sliderinstances.Frame_3.BorderSizePixel = 0
				sliderinstances.Frame_3.Position = UDim2.new(0, 1, 0, 1)
				sliderinstances.Frame_3.Size = UDim2.new(0, 237, 0, 11)
				sliderinstances.Frame_3.Name = 'Black'

				sliderinstances.Frame_4.Parent = sliderinstances.TextButton_1
				sliderinstances.Frame_4.BackgroundColor3 = CurrentTheme.ThemeColor
				sliderinstances.Frame_4.BorderSizePixel = 0
				sliderinstances.Frame_4.Position = UDim2.new(0, 1, 0, 1)
				sliderinstances.Frame_4.Selectable = true
				sliderinstances.Frame_4.Size = UDim2.new(0.987500011920929, 0, 0, 11)
				sliderinstances.Frame_4.Name = 'Green'

				sliderinstances.UICorner_3.Parent = sliderinstances.Frame_4
				sliderinstances.UICorner_3.CornerRadius = UDim.new(0, 2)

				local Val = sliderinstances.TextLabel_1
				local SliderFrame = sliderinstances.TextButton_1
				local MousePressed = false
				local Green = sliderinstances.Frame_4

				local held = false
				local step = 0
				local percentage = 0
				local function snap(number, factor)
					if factor == 0 then
						return number
					else
						return math.floor(number/factor+0.5)*factor
					end	
				end
				if Default and Default >= 0 then
					Green.Size = UDim2.new(Default / Max,-(Green.Size.X.Offset*0.5),0, 11)
					Val.Text = (math.floor((0 + (Max - 0) * math.clamp(Default / Max,0,1)) * Max) / Max)
				end

				SliderFrame.MouseButton1Down:Connect(function()
					held = true
					MousePressed = true

				end)

				game:GetService("UserInputService").InputEnded:Connect(function(input, gp)
					if input.UserInputType == Enum.UserInputType.MouseButton1 then
						held = false
						MousePressed = false
					end
				end)

				SliderFrame.MouseEnter:connect(function()
					repeat
						wait()
						if MousePressed then
							held = true
						end
					until Green.MouseLeave
				end)
				Green.MouseEnter:connect(function()
					repeat
						wait()
						if MousePressed then
							held = true
						end
					until SliderFrame.MouseLeave
				end)

				game:GetService("RunService").RenderStepped:connect(function(delta)
					if held then
						local MousePos = game:GetService("UserInputService"):GetMouseLocation().X
						local BtnPos = Green.Position
						local SliderSize = SliderFrame.AbsoluteSize.X
						local SliderPos = SliderFrame.AbsolutePosition.X
						local pos = snap((MousePos-SliderPos)/SliderSize,step)
						percentage = math.clamp(pos,0,1)
						game.TweenService:Create(Green, TweenInfo.new(0.05, Enum.EasingStyle.Linear, Enum.EasingDirection.In), {
							Size = UDim2.new(percentage,-(Green.Size.X.Offset*0.5),0, 11)
						}):Play()
						local bry = (math.floor((0 + (Max - 0) * percentage) * Max) / Max)
						local final = math.floor(bry)
						Val.Text = final
						callback(final)
						if Min >= final then
							Val.Text = Val.Text..Min
							callback(final)
						end
					end
				end)

			end

			function ElementHandler:CreateKeybind(config)
				local btnText = config.Text or config.text or "Keybind"
				local callback = config.callback or config.Callback or function() end   
				local DefaultKey = Enum.KeyCode[config.Default] or config.default or Enum.KeyCode.Q
				local OrderNumber = config.Order or config.order or 20


				if not DefaultKey then
					return
				end

				if not table.find(BlacklistedKeys,DefaultKey) then
					DefaultKey = DefaultKey
				else
					DefaultKey = Enum.KeyCode.Q
				end

				local bindinstances = {
					["TextLabel_1"] = Instance.new("TextLabel"),
					["Frame_2"] = Instance.new("Frame"),
					["Frame_1"] = Instance.new("Frame"),
					["UICorner_1"] = Instance.new("UICorner"),
					["TextLabel_2"] = Instance.new("TextLabel"),
					["TextButton_1"] = Instance.new("TextButton"),
					["LocalScript_1"] = Instance.new("LocalScript"),
				}

				bindinstances.Frame_1.Parent = sectioninstances.Frame_3
				bindinstances.Frame_1.BackgroundColor3 = Color3.new(1, 1, 1)
				bindinstances.Frame_1.BackgroundTransparency = 1
				bindinstances.Frame_1.BorderColor3 = Color3.new(0.105882, 0.164706, 0.207843)
				bindinstances.Frame_1.BorderSizePixel = 0
				bindinstances.Frame_1.LayoutOrder = 3
				bindinstances.Frame_1.Position = UDim2.new(0, 0, 0, 20) 
				bindinstances.Frame_1.Size = UDim2.new(0, 240, 0, 33)
				bindinstances.Frame_1.LayoutOrder = OrderNumber
				bindinstances.Frame_1.Name = 'Keybind'

				bindinstances.Frame_2.Parent = bindinstances.Frame_1
				bindinstances.Frame_2.BackgroundColor3 = Color3.new(0.0588235, 0.0588235, 0.0588235)
				bindinstances.Frame_2.BorderColor3 = Color3.new(0.27451, 0.27451, 0.27451)
				bindinstances.Frame_2.BorderSizePixel = 2
				bindinstances.Frame_2.Position = UDim2.new(0, 0, 0.3125, 0)
				bindinstances.Frame_2.Size = UDim2.new(0, 33, 0, 16)
				bindinstances.Frame_2.Name = 'Outline'

				bindinstances.TextButton_1.Parent = bindinstances.Frame_2
				bindinstances.TextButton_1.Font = Enum.Font.SourceSans
				bindinstances.TextButton_1.Text = ''
				bindinstances.TextButton_1.TextColor3 = Color3.new(0, 0, 0)
				bindinstances.TextButton_1.AutoButtonColor = false
				bindinstances.TextButton_1.TextSize = 14
				bindinstances.TextButton_1.BackgroundColor3 = CurrentTheme.ThemeColor
				bindinstances.TextButton_1.BorderSizePixel = 0
				bindinstances.TextButton_1.Position = UDim2.new(-0.06300048530101776, 0, -0.06299972534179688, 0)
				bindinstances.TextButton_1.Size = UDim2.new(0, 36, 0, 18)

				bindinstances.UICorner_1.Parent = bindinstances.TextButton_1
				bindinstances.UICorner_1.CornerRadius = UDim.new(0, 2)

				bindinstances.TextLabel_1.Parent = bindinstances.TextButton_1
				bindinstances.TextLabel_1.Font = Enum.Font.Gotham
				bindinstances.TextLabel_1.Text = DefaultKey.Name
				bindinstances.TextLabel_1.TextColor3 = CurrentTheme.TextColor
				bindinstances.TextLabel_1.TextScaled = true
				bindinstances.TextLabel_1.TextSize = 15
				bindinstances.TextLabel_1.TextWrapped = true
				bindinstances.TextLabel_1.BackgroundColor3 = Color3.new(1, 1, 1)
				bindinstances.TextLabel_1.BackgroundTransparency = 1
				bindinstances.TextLabel_1.Size = UDim2.new(1, 0, 0.949999988079071, 0)
				bindinstances.TextLabel_1.Position = UDim2.fromOffset(0,1)

				bindinstances.LocalScript_1.Parent = bindinstances.TextButton_1

				bindinstances.TextLabel_2.Parent = bindinstances.Frame_1
				bindinstances.TextLabel_2.Font = Enum.Font.Gotham
				bindinstances.TextLabel_2.Text = btnText
				bindinstances.TextLabel_2.TextColor3 = CurrentTheme.TextColor
				bindinstances.TextLabel_2.TextSize = 14
				bindinstances.TextLabel_2.TextXAlignment = Enum.TextXAlignment.Left
				bindinstances.TextLabel_2.BackgroundColor3 = Color3.new(1, 1, 1)
				bindinstances.TextLabel_2.BackgroundTransparency = 1
				bindinstances.TextLabel_2.Position = UDim2.new(0.18299999833106995, 0, 0.2800000011920929, 0)
				bindinstances.TextLabel_2.Size = UDim2.new(0, 76, 0, 22)

				local Shadow = Instance.new("Frame")

				Shadow.Name = "Shadow"
				Shadow.Parent = bindinstances.TextButton_1
				Shadow.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
				Shadow.BackgroundTransparency = 1
				Shadow.BorderColor3 = Color3.fromRGB(27, 42, 53)
				Shadow.BorderSizePixel = 0
				Shadow.Position = UDim2.new(8.4771051e-07, 0, 0, 0)
				Shadow.Size = UDim2.new(0, 36, 0, 20)
				Shadow.ZIndex = 2


				bindinstances.TextButton_1.MouseEnter:Connect(function()
					game.TweenService:Create(bindinstances.TextButton_1:FindFirstChild("Shadow"), TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.In), {
						BackgroundTransparency = 0.8
					}):Play()
				end)
				bindinstances.TextButton_1.MouseLeave:Connect(function()
					game.TweenService:Create(bindinstances.TextButton_1:FindFirstChild("Shadow"), TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.In), {
						BackgroundTransparency = 1
					}):Play()
				end)		

				local Key = DefaultKey
				local Listening = false
				game:GetService("UserInputService").InputBegan:Connect(function(Input, IsTyping)
					if IsTyping then return end
					if Input.KeyCode == Key then
						callback()
					end
					if Listening == true then
						if not table.find(BlacklistedKeys,Input.KeyCode) then
							Key = Input.KeyCode
						else
							Key = Enum.KeyCode.Q
						end

						Listening = false
						if Key.Name == "LeftControl" then
							bindinstances.TextLabel_1.Text = "LControl"
						elseif Key.Name == "RightControl" then
							bindinstances.TextLabel_1.Text = "RControl"
						elseif Key.Name == "LeftShift" then
							bindinstances.TextLabel_1.Text = "LShift"
						elseif Key.Name == "RightShift" then
							bindinstances.TextLabel_1.Text = "RShift"
						else
							bindinstances.TextLabel_1.Text = Key.Name
						end

					end
				end)

				bindinstances.TextButton_1.MouseButton1Click:Connect(function()
					Listening = true
					bindinstances.TextLabel_1.Text = ".."
				end)

			end

			function ElementHandler:CreateDropdown(config)
				local DropdownFunction = {}
				local btnText = config.Text or config.text or "Dropdown"
				local callback = config.callback or config.Callback or function() end   
				local array = config.array or config.Array or {}
				local opened
				local OrderNumber = config.Order or config.order or 20

				local dropdown = {
					["UICorner_1"] = Instance.new("UICorner"),
					["ImageLabel_1"] = Instance.new("ImageLabel"),
					["Frame_2"] = Instance.new("Frame"),
					["UIListLayout_1"] = Instance.new("UIListLayout"),
