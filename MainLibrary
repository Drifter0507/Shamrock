			game.TweenService:Create(TabInstances.TextButton_1:FindFirstChild("Frame"), TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.In), {
				Size = UDim2.new(0.25, 0, 0.07407407462596893, 0)
			}):Play()
		end)


