		TabInstances.TextButton_1.Position = UDim2.new(0.08771929889917374, 0, 0.05017921328544617, 0)
		TabInstances.TextButton_1.Size = UDim2.new(0, 104, 0, 27)
		TabInstances.TextButton_1.Name = tabname

		TabInstances.PageFrame.Parent = TabInstances.TextButton_1
		--TabInstances.PageFrame.BackgroundColor3 = Color3.new(0, 0.631373, 0.294118)
		TabInstances.PageFrame.BorderSizePixel = 0
		--TabInstances.PageFrame.Position = UDim2.new(0, 0, 0, 24)
		--TabInstances.PageFrame.Size = UDim2.new(0.25, 0, 0.07407407462596893, 0)
		if taken == false then
