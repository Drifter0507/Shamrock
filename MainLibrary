				dropdown.Frame_3:FindFirstChildOfClass("UIListLayout"):GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
					local size = dropdown.Frame_3:FindFirstChildOfClass("UIListLayout").AbsoluteContentSize
					local padding = dropdown.Frame_3:FindFirstChildOfClass("UIListLayout").Parent.UIPadding

					size = size + Vector2.new(0, padding.PaddingBottom.Offset + (padding.PaddingTop.Offset))
					game.TweenService:Create(dropdown.Frame_3, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.In), {
						Size = UDim2.new(0, 236, 0, size.Y + 2)
					}):Play()
					game.TweenService:Create(dropdown.Frame_1, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.In), {
						Size = UDim2.new(0, 240, 0, size.Y + 33)
					}):Play()
				end)

				dropdown.TextButton_1.MouseButton1Click:Connect(function()
					--UpdateList()
					if opened then
						game.TweenService:Create(dropdown.ImageLabel_1, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.In), {
							Rotation = 0
						}):Play()
						opened = false
						for i, v in pairs(dropdown.Frame_3:GetChildren()) do
							if v:IsA("TextButton") then
								game.TweenService:Create(v, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.In), {
									Size = UDim2.new(0, 236, 0, 0)
								}):Play()
								game.TweenService:Create(v:FindFirstChildOfClass("TextLabel"), TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.In), {
									TextTransparency = 1
								}):Play()
								--v.Visible = false
							end
						end
						dropdown.Frame_3.BackgroundTransparency = 1
					else
						game.TweenService:Create(dropdown.ImageLabel_1, TweenInfo.new(0.2, Enum.EasingStyle.Linear, Enum.EasingDirection.In), {
							Rotation = -180
						}):Play()
						opened = true
						for i, v in pairs(dropdown.Frame_3:GetChildren()) do
							if v:IsA("TextButton") then
								--v.Visible = true
								game.TweenService:Create(v, TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.In), {
									Size = UDim2.new(0, 236, 0, 15)
								}):Play()
								game.TweenService:Create(v:FindFirstChildOfClass("TextLabel"), TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.In), {
									TextTransparency = 0
								}):Play()
							end
						end
						dropdown.Frame_3.BackgroundTransparency = 1
					end
				end)

				dropdown.TextButton_1.MouseEnter:Connect(function()
